<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.video.nzbsu"
       name="Nzb.su"
       version="0.0.3"
       provider-name="Popeye">
  <requires>
    <import addon="xbmc.python" version="1.0"/>
    <import addon="plugin.video.nzbs" version="0.0.8"/>
  </requires>
  <extension point="xbmc.python.pluginsource"
            library="default.py">
        <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <platform>all</platform>
    <summary>Nzb.su</summary>
    <description>Browse and stream from Nzb.su</description>
    <disclaimer>Requires the NZBS add-on</disclaimer>
  </extension>
</addon>